<table>
    <thead>
        <tr>
            <th scope='col'>Name</th>
            <th scope='col'>Chemical</th>
        </tr>
    </thead>
    <tbody>
        <% @unregistered_products.each do |product| %>
            <tr>
                <td><%= product.product_name %></td>
                <td><%= product.chemical %></td>
            </tr>
        <% end %>
    </tbody>
</table>
<%= paginate @unregistered_products, remote: true ,param_name: :unregistered_page_no %>